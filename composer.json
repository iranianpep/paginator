{
  "name": "paginator/paginator",
  "description": "A packagist ready package to start a new PHP 7 project including Composer, PHPUnit, Travis, CodeClimate and StyleCI.",
  "homepage": "https://github.com/iranianpep/paginator",
  "license": "MIT",
  "authors": [
    { "name": "Ehsan Abbasi", "email": "ehsan.abb@gmail.com" }
  ],
  "require": {
    "php": ">=7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6"
  },
  "autoload": {
    "psr-4": {
      "Paginator\\": ["src/Paginator"]
    }
  },
  "autoload-dev": {
    "psr-4": {"Paginator\\": "tests/Paginator"}
  }
}
